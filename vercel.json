{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "OneLastMerch/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "OneLastMerch.settings"
  }
}
