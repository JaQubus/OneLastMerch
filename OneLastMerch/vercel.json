{
  "version": 2,
  "builds": [
    {
      "src": "OneLastMerch/wsgi.py",
      "use": "@vercel/python",
      "buildCommand": "python manage.py collectstatic --noinput",
      "config": {
        "maxDuration": 60
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/OneLastMerch/wsgi.py"
    }
  ]
}
