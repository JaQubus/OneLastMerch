{
  "version": 2,
  "builds": [
    {
      "src": "OneLastMerch/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxDuration": 60
      }
    }
  ],
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "OneLastMerch.settings"
    },
    "commands": [
      "python manage.py collectstatic --noinput"
    ]
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/OneLastMerch/wsgi.py"
    }
  ]
}
