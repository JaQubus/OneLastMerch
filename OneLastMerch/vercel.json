{
  "version": 2,
  "builds": [
    {
      "src": "OneLastMerch/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": ""
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/OneLastMerch/wsgi.py"
    }
  ]
}
